<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
			<solid android:color="#00000000"/>
			<stroke android:width="0.5dp" android:color="@color/common_btn_reverse_enable"/>
			<corners android:radius="4dp"/>
			<padding android:left="8dp" android:right="8dp" android:top="4dp"  android:bottom="4dp"></padding>
		</shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dp" android:color="@color/common_btn_reverse_normal"/>
			<solid android:color="@color/common_btn_reverse_pressed"/>
			<corners android:radius="4dp"/>
			<padding android:left="8dp" android:right="8dp" android:top="4dp"  android:bottom="4dp"></padding>
		</shape>
    </item>
    <item>
        <shape>
			<solid android:color="#00000000"/>
			<stroke android:width="0.5dp" android:color="@color/common_btn_reverse_normal"/>
			<corners android:radius="4dp"/>
			<padding android:left="8dp" android:right="8dp" android:top="4dp"  android:bottom="4dp"></padding>
		</shape>
    </item>
</selector>

