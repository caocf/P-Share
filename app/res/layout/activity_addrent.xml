<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical" >
    <RelativeLayout
        android:layout_width="match_parent" 
        android:layout_height="@dimen/padding8"       
        android:background="@color/bg_action_bar_blue">
        <Button
            android:id="@+id/activity_back" 
            android:layout_height="@dimen/padding8"
            android:layout_width="@dimen/padding8"
            android:padding="@dimen/padding2"
            android:background="@null"
            android:drawableLeft="@drawable/left_head"/>
        <TextView 
            style="@style/text_actionbar"
            android:text="@string/add_rent"/>
    </RelativeLayout>
    <ScrollView 
        android:layout_height="wrap_content"
        android:layout_width="wrap_content">
        <LinearLayout 
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/rl_city" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"       
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                
	                android:layout_centerVertical="true"
	                android:text="@string/tv_city"/>
	            <TextView
	                android:id="@+id/tv_city" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_city"
	                android:layout_centerVertical="true"
	                android:textColor="@color/text_light_gray"
	                android:layout_marginRight="@dimen/padding1"
	                android:text="中国  上海  上海"/>
	            <ImageView
	                android:id="@+id/iv_city" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:layout_centerVertical="true"
	                android:src="@drawable/black_right_head"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_area" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"       
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_area"/>
	            <TextView
	                android:id="@+id/tv_area" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_area"
	                android:layout_centerVertical="true"
	                android:textColor="@color/text_light_gray"
	                android:layout_marginRight="@dimen/padding1"
	                android:text="长宁区"/>
	            <ImageView
	                android:id="@+id/iv_area" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:layout_centerVertical="true"
	                android:src="@drawable/black_right_head"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_living_area" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="#ffffff">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_living_area"/>
	            <TextView
	                android:id="@+id/tv_living_area" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_living_area"
	                android:layout_centerVertical="true"
	                android:textColor="@color/text_light_gray"
	                android:layout_marginRight="@dimen/padding1"
	                android:text="金龟小区"/>
	            <ImageView
	                android:id="@+id/iv_living_area" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:layout_centerVertical="true"
	                android:src="@drawable/black_right_head"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_time_frame" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_time_frame"/>
	            <TextView
	                android:id="@+id/tv_time_frame" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_time_frame"
	                android:layout_centerVertical="true"
	                android:textColor="@color/text_light_gray"
	                android:layout_marginRight="@dimen/padding1"/>
	            <ImageView
	                android:id="@+id/iv_time_frame" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:layout_centerVertical="true"
	                android:src="@drawable/black_right_head"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_start_time" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_start_time"/>
	            <TextView
	                android:id="@+id/tv_start_time" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_start_time"
	                android:layout_centerVertical="true"
	                android:textColor="@color/text_light_gray"
	                android:layout_marginRight="@dimen/padding1"/>
	            <ImageView
	                android:id="@+id/iv_start_time" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:layout_centerVertical="true"
	                android:src="@drawable/black_right_head"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_finish_time" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_finish_time"/>
	            <TextView
	                android:id="@+id/tv_finish_time" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/padding5"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_tv_price" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_price"/>
	            <TextView
	                android:id="@+id/tv_price" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/padding5"
	                android:text="100元/月"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_name" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_name"/>
	            <EditText
	                android:id="@+id/EditText_name"
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:textColor="@color/text_light_gray"
	                android:background="@drawable/edittext_time"
	                android:textSize="@dimen/text_size3"
	                android:gravity="right"
	                android:hint="车"
	                android:layout_marginRight="@dimen/padding5"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_tv_car_brand" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_car_brand"/>
	            <TextView
	                android:id="@+id/tv_car_brand" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_car_brand"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/padding1"/>
	            <ImageView
	                android:id="@+id/iv_car_brand" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:layout_centerVertical="true"
	                android:src="@drawable/black_right_head"/>
        	</RelativeLayout>
        	<Button
        	    android:id="@+id/button_confirm" 
        	    style="@style/button_style"
        	    android:text="@string/confirm"/>
      	</LinearLayout>       
    </ScrollView>

</LinearLayout>
