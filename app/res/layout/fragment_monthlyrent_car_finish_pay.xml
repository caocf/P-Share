    <ScrollView 
        android:layout_height="wrap_content"
        android:layout_width="wrap_content">
        <LinearLayout 
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="vertical">
			<RelativeLayout
				android:id="@+id/rl_CPC"
				android:layout_height="@dimen/padding7"
				android:layout_width="match_parent"
				android:background="@color/text_white">
				<TextView
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_marginLeft="@dimen/padding2"
					android:textSize="@dimen/text_size3"
					android:layout_centerVertical="true"
					android:text="@string/tv_city"/>
				<TextView
					android:id="@+id/tv_country"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_toLeftOf="@+id/tv_province"
					android:layout_alignParentRight="true"
					android:layout_centerVertical="true"
					android:textColor="@color/text_light_gray"
					android:layout_marginRight="@dimen/padding1"
					android:text="中国"/>
				<!--<TextView
					android:id="@+id/tv_province"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_toLeftOf="@+id/tv_city"
					android:layout_centerVertical="true"
					android:textColor="@color/text_light_gray"
					android:layout_marginRight="@dimen/padding1"
					android:text="上海"/>
				<TextView
					android:id="@+id/tv_city"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_centerVertical="true"
					android:layout_alignParentRight="true"
					android:textColor="@color/text_light_gray"
					android:layout_marginRight="@dimen/padding1"
					android:text="上海"/>-->
			</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_area" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"       
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_area"/>
	            <TextView
	                android:id="@+id/tv_area" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_area"
	                android:layout_centerVertical="true"
	                android:layout_alignParentRight="true"
	                android:textColor="#999d9c"
	                android:layout_marginRight="@dimen/padding1"
	                android:text="长宁区"/>	            
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_living_area" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_living_area"/>
	            <TextView
	                android:id="@+id/tv_living_area" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_toLeftOf="@+id/iv_living_area"
	                android:textColor="@color/text_light_gray"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:text="金龟小区"/>	            
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_time_frame" 
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_time_frame"/>
	            <TextView
	                android:id="@+id/tv_time_frame" 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"	                
	                android:textColor="@color/text_light_gray"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/padding1"/>	           
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_monthlyrent_begint_time"
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_start_time"/>
	            <TextView
	                android:id="@+id/tv_monthlyrent_begint_time"
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:textColor="@color/text_light_gray"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/padding1"/>	           
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_monthlyrent_end_time"
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_finish_time"/>
	            <TextView
	                android:id="@+id/tv_monthlyrent_end_time"
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
					android:textColor="@color/text_light_gray"
	                android:layout_marginRight="@dimen/padding1"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_monthlyrent_price"
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_price"/>
	            <TextView
	                android:id="@+id/tv_monthlyrent_price"
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
					android:textColor="#999d9c"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/padding1"
	                android:text="100元/月"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_customer_name"
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_name"/>
	            <TextView
	                android:id="@+id/tv_customer_name"
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:background="@drawable/edittext_time"
	                android:textSize="@dimen/text_size3"
	                android:gravity="right"
					android:textColor="@color/text_light_gray"	                android:hint="车"
	                android:layout_marginRight="@dimen/padding1"/>
        	</RelativeLayout>
        	<RelativeLayout
        	    android:id="@+id/rl_tv_car_number"
	            android:layout_height="@dimen/padding7"
	            android:layout_width="match_parent"
	            android:layout_marginTop="@dimen/padding10"        
	            android:background="@color/text_white">
	            <TextView 
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"
	                android:layout_marginLeft="@dimen/padding2"
	                android:textSize="@dimen/text_size3"
	                android:layout_centerVertical="true"
	                android:text="@string/tv_car_brand"/>
	            <TextView
	                android:id="@+id/tv_car_number"
	                android:layout_height="wrap_content"
	                android:layout_width="wrap_content"	                
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
					android:textColor="@color/text_light_gray"
					android:layout_marginRight="@dimen/padding1"/>
        	</RelativeLayout>
        	<Button
        	    android:id="@+id/button_finish_pay"
        	    android:layout_height="@dimen/padding7"
        	    android:layout_width="match_parent"
        	    android:background="@drawable/finish_button"
        	    android:textSize="@dimen/text_size5"
        	    android:layout_margin="@dimen/padding4"
        	    android:textColor="@color/text_white"
        	    android:text="已支付"/>
      	</LinearLayout>       
    </ScrollView>

</LinearLayout>

