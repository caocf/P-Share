<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#121a2a"
    android:orientation="vertical" >
    <LinearLayout 
        android:layout_width="match_parent"
	    android:layout_height="match_parent"	   
	    android:orientation="vertical">
	    <!--  设置-->
        <ImageView
            android:id="@+id/iv_slidingmenu_install"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/padding2"
            android:layout_marginTop="@dimen/padding2"
           	android:src="@drawable/iv_slidingmenu_install"
            android:gravity="center"/>
        <!--  个人中心-->
        <LinearLayout
            android:id="@+id/personal_center"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:background="@null"
	        android:orientation="vertical" >
	        <ImageView
	            android:id="@+id/iv_slidingmenu_userhead"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_horizontal"
	            android:padding="@dimen/padding2"
	            android:src="@drawable/userhead"/>	
	        <TextView
	            android:id="@+id/tv_slidingmenu_userName"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:textColor="#ffffff"
	            android:text="车先生"
	            android:textSize="@dimen/text_size3"/>	
	        <TextView
	            android:id="@+id/tv_slidingmenu_userID"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_horizontal"
	            android:layout_margin="@dimen/padding1"
	            android:gravity="center"
	            android:textColor="#ffffff"
	            android:text="ID:1234444444"
	            android:textSize="@dimen/text_size3"/>
	        <LinearLayout 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_horizontal"
	            android:layout_margin="@dimen/padding1"
	            android:orientation="horizontal"
	            android:gravity="center">
	            <TextView		            
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:layout_margin="@dimen/padding1"
		            android:gravity="center"
		            android:textColor="@color/text_white"
		            android:text="积分："
		            android:textSize="@dimen/text_size3"/>
		         <TextView
		            android:id="@+id/tv_slidingmenu_userIntegration"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:layout_margin="@dimen/padding1"
		            android:gravity="center"
		            android:textColor="#fcaf17"
		            android:text="0"
		            android:textSize="@dimen/text_size3"/>
	        </LinearLayout>	         
       </LinearLayout>
       <!--  分割线-->
       <TextView 
           android:layout_height="1dp"
           android:layout_width="match_parent"
           android:layout_marginTop="@dimen/padding4"
           android:background="#f8f8ff"/>
       <!--  代泊区-->
       <RelativeLayout
            android:id="@+id/rl_slidingmenu_ParkingArea"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding2"
         	android:layout_marginBottom="@dimen/padding2" >
         	<ImageView
         	    android:id="@+id/imageView1" 
         	    android:layout_width="wrap_content"
         	    android:layout_height="wrap_content"
         	    android:src="@drawable/iv_slidingmenu_parking_area"
         	    android:layout_centerVertical="true"
         	    android:layout_marginLeft="@dimen/padding4"
         	    />
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_toRightOf="@+id/imageView1"
                android:layout_marginLeft="@dimen/padding2"
                android:layout_centerVertical="true"
                android:textColor="#ffffff"
                android:textSize="@dimen/text_size4"
                android:text="@string/parking_area"/>            
            <ImageView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/padding4"              
                android:src="@drawable/iv_slidingmenu_righthead"/>
        </RelativeLayout>
        <!--  分割线-->
        <View style="@style/view_line"/>
        <!--  我的预约/订单-->		    
        <RelativeLayout
	         android:id="@+id/rl_slidingmenu_order"
	         android:layout_width="match_parent"
	         android:layout_height="wrap_content"
	         android:layout_marginTop="@dimen/padding2"
	      	 android:layout_marginBottom="@dimen/padding2" >
	      	<ImageView
	      	    android:id="@+id/imageView3" 
	      	    android:layout_width="wrap_content"
	      	    android:layout_height="wrap_content"
	      	    android:src="@drawable/iv_slidingmenu_order"
	      	    android:layout_centerVertical="true"
	      	    android:layout_marginLeft="@dimen/padding4"
	      	    />
	         <TextView
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:layout_toRightOf="@+id/imageView3"
	             android:layout_marginLeft="@dimen/padding2"
	             android:textColor="#ffffff"
	             android:layout_centerVertical="true"
	             android:textSize="@dimen/text_size4"
	             android:text="@string/order" />            
	         <ImageView
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:layout_alignParentRight="true"
	             android:layout_centerVertical="true"
	             android:layout_marginRight="@dimen/padding4"
	             android:src="@drawable/iv_slidingmenu_righthead"/>
	     </RelativeLayout>
	     <!--  分割线-->
	     <View style="@style/view_line"/>
	     <!--  我的车列表--> 
	     <RelativeLayout
		      android:id="@+id/rl_slidingmenu_carList"
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_marginTop="@dimen/padding2"
		      android:layout_marginBottom="@dimen/padding2" >
		   	<ImageView
		   	    android:id="@+id/imageView4" 
		   	    android:layout_width="wrap_content"
		   	    android:layout_height="wrap_content"
		   	    android:layout_centerVertical="true"
		   	    android:src="@drawable/iv_slidingmenu_carlist"
		   	    android:layout_marginLeft="@dimen/padding4"/>
	      <TextView
	          android:layout_height="wrap_content"
	          android:layout_width="wrap_content"
	          android:layout_toRightOf="@+id/imageView4"
	          android:layout_marginLeft="@dimen/padding2"
	          android:layout_centerVertical="true"
	          android:textColor="#ffffff"
	          android:textSize="@dimen/text_size4"
	          android:text="@string/mine_carList" />            
	      <ImageView
	          android:layout_height="wrap_content"
	          android:layout_width="wrap_content"
	          android:layout_alignParentRight="true"
	          android:layout_centerVertical="true"
	          android:layout_marginRight="@dimen/padding4"
	          android:src="@drawable/iv_slidingmenu_righthead"/>
		  </RelativeLayout>
		  <!--  分割线-->
		 <View style="@style/view_line"/>
		 <!--  我的月租列表-->
		 <RelativeLayout
		      android:id="@+id/rl_slidingmenu_monthlyRent"
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_marginTop="@dimen/padding2"
		      android:layout_marginBottom="@dimen/padding2" >
		   	<ImageView
		   	    android:id="@+id/imageView5" 
		   	    android:layout_width="wrap_content"
		   	    android:layout_height="wrap_content"
		   	    android:layout_centerVertical="true"
		   	    android:src="@drawable/iv_slidingmenu_monthly_rent"
		   	    android:layout_marginLeft="@dimen/padding4"/>
	      <TextView
	          android:layout_height="wrap_content"
	          android:layout_width="wrap_content"
	          android:layout_toRightOf="@+id/imageView5"
	          android:layout_marginLeft="@dimen/padding2"
	          android:layout_centerVertical="true"
	          android:textColor="#ffffff"
	          android:textSize="@dimen/text_size4"
	          android:text="@string/monthly_rent" />            
	      <ImageView
	          android:layout_height="wrap_content"
	          android:layout_width="wrap_content"
	          android:layout_alignParentRight="true"
	          android:layout_centerVertical="true"
	          android:layout_marginRight="@dimen/padding4"
	          android:src="@drawable/iv_slidingmenu_righthead"/>
		  </RelativeLayout>
		  <!--  分割线-->
		 <View style="@style/view_line"/>
		 <!--  优惠/积分-->
		 <RelativeLayout
            android:id="@+id/rl_slidingmenu_integration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding2"
         	android:layout_marginBottom="@dimen/padding2" >
         	<ImageView
         	    android:id="@+id/imageView2" 
         	    android:layout_width="wrap_content"
         	    android:layout_height="wrap_content"
         	    android:src="@drawable/iv_slidingmenu_integration"
         	    android:layout_centerVertical="true"
         	    android:layout_marginLeft="@dimen/padding4"
         	    />
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_toRightOf="@+id/imageView2"
                android:layout_marginLeft="@dimen/padding2"
                android:layout_centerVertical="true"
                android:textColor="#ffffff"
                android:textSize="@dimen/text_size4"
                android:text="@string/integration"/>            
            <ImageView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/padding4"              
                android:src="@drawable/iv_slidingmenu_righthead"/>
        </RelativeLayout>
        <!--  分割线-->
        <View style="@style/view_line"/>
    </LinearLayout>     	
</ScrollView>
