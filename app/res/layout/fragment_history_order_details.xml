<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/fragment_background">
    <LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:background="@color/bg_color"
	    android:orientation="vertical" >
		<TextView
			android:id="@+id/tv_you_servicePerson"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:padding="@dimen/padding2"
			android:textSize="@dimen/text_size4"
			android:textColor="@color/rb_text_color_gray"
			android:text="@string/tv_you_servicePerson"/>
		<RelativeLayout
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:layout_below="@+id/tv_you_servicePerson"
			android:background="@color/text_white">
			<ImageView
				android:id="@+id/imageView1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/padding4"
				android:background="@drawable/order_car_bg"/>
			<!-- 代泊员头像  -->
			<com.azhuoinfo.pshare.view.imageview.round.RoundedImageView
				android:id="@+id/iv_parker_head"
				android:scaleType="centerCrop"
				app:oval="true"
				android:layout_height="100dp"
				android:layout_width="100dp"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="@dimen/padding2"
				android:src="@drawable/userhead"/>
			<!-- 代泊员号  -->
			<LinearLayout
				android:id="@+id/ll_parker_id"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="@dimen/padding1"
				android:layout_below="@+id/iv_parker_head"
				android:orientation="horizontal">
				<TextView
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_marginTop="@dimen/padding2"
					android:textSize="@dimen/text_size3"
					android:text="@string/tv_order_servicePerson" />
				<TextView
					android:id="@+id/tv_parker_id"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_marginTop="@dimen/padding2"
					android:text="2"
					android:textSize="@dimen/text_size3" />
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_marginTop="@dimen/padding2"
					android:text="号"
					android:textSize="@dimen/text_size3" />
			</LinearLayout>
			<!-- 代泊员的职务 -->
			<LinearLayout
				android:id="@+id/ll_parker_position"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="@dimen/padding1"
				android:layout_below="@+id/ll_parker_id"
				android:orientation="horizontal">
				<TextView
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:textSize="@dimen/text_size3"
					android:text="@string/tv_order_servicePerson_position"/>
				<TextView
					android:id="@+id/tv_parker_position"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:textSize="@dimen/text_size3"
					android:text="@string/text_order_servicePerson_position"/>
			</LinearLayout>
			<!-- 代泊员负责的区域地点 -->
			<LinearLayout
				android:id="@+id/ll_parker_area"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="@dimen/padding1"
				android:layout_below="@+id/ll_parker_position"
				android:orientation="horizontal">
				<TextView
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:textSize="@dimen/text_size3"
					android:text="@string/tv_order_servicePerson_area"/>
				<TextView
					android:id="@+id/tv_parker_area"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:textSize="@dimen/text_size3"
					android:text="@string/text_order_servicePerson_area"/>
			</LinearLayout>
			<!-- 拨打代泊员号码 -->
			<RelativeLayout
				android:id="@+id/rl_parker_mobile"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="@dimen/padding1"
				android:layout_below="@+id/ll_parker_area">
				<LinearLayout
					android:id="@+id/ll_parker_mobile"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:orientation="horizontal">
					<ImageView
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:src="@drawable/call_phone"/>
					<TextView
						android:id="@+id/tv_parker_mobile"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="@dimen/padding1"
						android:textColor="@color/text_phone"
						android:text="18301969764"/>
				</LinearLayout>
				<View
					android:layout_height="1dp"
					android:layout_width="match_parent"
					android:layout_alignParentLeft="@+id/ll_parker_mobile"
					android:layout_alignRight="@+id/ll_parker_mobile"
					android:background="@color/text_phone"
					android:layout_marginTop="@dimen/padding1"
					android:layout_below="@+id/ll_parker_mobile"/>
			</RelativeLayout>
	    </RelativeLayout>
	    <!-- 订单时间 -->
	     <TextView
	        android:id="@+id/tv_user_Stop_state" 
	        android:layout_height="wrap_content"
	        android:layout_width="wrap_content"
	        android:padding="@dimen/padding2"
	        android:textSize="@dimen/text_size4"
	        android:textColor="@color/rb_text_color_gray"
	        android:text="@string/tv_user_stop_state"/>
	     <!-- 停车时长 -->
	     <LinearLayout
	         android:id="@+id/ll_history_order_time"
	         android:layout_height="wrap_content"
	         android:layout_width="match_parent"
	         android:padding="@dimen/padding2"	        
	         android:background="@color/text_white">
	         <TextView 
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:text="@string/tv_history_order_time"/>
	         <TextView
	             android:id="@+id/tv_history_order_time"
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:text="@string/text_history_order_time"/>
	     </LinearLayout>
	     <!--  停车时长-->
	     <LinearLayout
	         android:id="@+id/ll_stop_car_time"
	         android:layout_height="wrap_content"
	         android:layout_width="match_parent"
	         android:padding="@dimen/padding2"	        
	         android:background="@color/text_white">
	         <TextView 
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:text="@string/tv_stop_car_time"/>
	         <TextView
	             android:id="@+id/tv_stop_car_time"
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:text="@string/text_stop_car_time"/>
	     </LinearLayout>
	     <!--  支付金额-->
	     <LinearLayout
	         android:id="@+id/ll_pay_money"
	         android:layout_height="wrap_content"
	         android:layout_width="match_parent"
	         android:padding="@dimen/padding2"	        
	         android:background="@color/text_white">
	         <TextView 
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:text="@string/tv_pay_money"/>
	         <TextView
	             android:id="@+id/tv_pay_money"
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:textColor="@color/text_color_red"/>
			 <TextView
				 android:layout_height="wrap_content"
				 android:layout_width="wrap_content"
				 android:textSize="@dimen/text_size3"
				 android:text="元"/>
	     </LinearLayout>
	     <!--  车牌号-->
	     <LinearLayout
	         android:id="@+id/ll_car_number"
	         android:layout_height="wrap_content"
	         android:layout_width="match_parent"
	         android:padding="@dimen/padding2"	        
	         android:background="@color/text_white">
	         <TextView 
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:text="@string/tv_order_car_number"/>
	         <TextView
	             android:id="@+id/tv_car_number"
	             android:layout_height="wrap_content"
	             android:layout_width="wrap_content"
	             android:textSize="@dimen/text_size3"
	             android:text="@string/text_order_car_number"/>
	     </LinearLayout>
		<Button
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@color/text_light_gray"
			android:layout_margin="50dp"
			android:text="订单取消"
			android:textColor="@color/text_white"
			android:visibility="gone"
			/>

		<RelativeLayout
			android:paddingTop="25dp"
			android:paddingBottom="25dp"
			android:layout_width="match_parent"
			android:layout_height="@dimen/padding12"
			android:background="@color/text_white"
			android:id="@+id/container"
			android:clipChildren="false"
			android:gravity="center"
			android:layerType="software"
			android:orientation="horizontal" >
			<ImageView
				android:id="@+id/iv_left"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:padding="10dp"
				android:layout_centerVertical="true"
				android:src="@drawable/order_left"/>
			<GridView
				android:id="@+id/gridview_photos"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_toRightOf="@+id/iv_left"
				android:layout_toLeftOf="@+id/iv_right"
				android:background="@color/text_white"
				android:scrollbars="none"
				android:numColumns="3"
				android:clipChildren="false">
			</GridView>
			<ImageView
				android:id="@+id/iv_right"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:padding="10dp"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:src="@drawable/order_right"/>
		</RelativeLayout>

